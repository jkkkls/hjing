{
  "name": "test-admin",
  "version": "5.2.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "max setup && npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "openapi": "max openapi",
    "playwright": "playwright install && playwright test",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^2.0.26",
    "@ant-design/pro-form": "^2.2.17",
    "@ant-design/pro-layout": "^7.2.1",
    "@ant-design/pro-table": "^3.1.4",
    "@umijs/max": "^4.0.29",
    "@umijs/route-utils": "^3.0.0",
    "ace-builds": "^1.31.2",
    "antd": "^4.24.1",
    "classnames": "^2.3.2",
    "lodash": "^4.17.0",
    "moment": "^2.29.4",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.7.1",
    "rc-util": "^5.24.2",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.2.0",
    "react-html-table-to-excel": "^2.0.0",
    "umi-presets-pro": "^1.0.8",
    "umi-serve": "^1.9.11"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.0.1",
    "@playwright/test": "^1.27.1",
    "@types/express": "^4.17.14",
    "@types/history": "^5.0.0",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^3.0.0",
    "@umijs/openapi": "^1.7.1",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "express": "^4.18.2",
    "gh-pages": "^4.0.0",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.0",
    "puppeteer-core": "^19.2.2",
    "stylelint": "^14.14.1",
    "swagger-ui-react": "^4.15.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
